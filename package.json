{
  "name": "fc-build-link",
  "version": "0.0.1",
  "description": "This is a component demo for Serverless Devs Tool ",
  "autoInstall": false,
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "Function Compute"
  ],
  "scripts": {
    "start": "npm run watch",
    "watch": "tsc -w",
    "prebuild": "rimraf doc && rimraf package-lock.json && rimraf dist && rimraf node_modules && npm i",
    "build": "npm run fix && npm run lint && ncc build src/index.ts -m -e @serverless-devs/core -o dist",
    "postbuild": "npm run doc",
    "doc": "npx typedoc src/index.ts --json doc/doc.json --out doc",
    "lint": "f2elint scan",
    "fix": "f2elint fix"
  },
  "dependencies": {
    "@serverless-devs/core": "^0.0.*"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "@vercel/ncc": "^0.24.0",
    "f2elint": "^1.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typedoc": "^0.20.35",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "f2elint commit-file-scan",
      "commit-msg": "f2elint commit-msg-scan"
    }
  }
}